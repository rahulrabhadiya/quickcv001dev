<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.googlel.liferay">
	<author>rahul</author>
	<namespace>googleplugins</namespace>
	<entity name="Employee" local-service="true"
		remote-service="false">

		<!-- PK fields -->

		<column name="employeeId" type="long" primary="true" id-type="increment"></column>

		<!-- Group instance -->

		<!-- Audit fields -->
        <column name="empName" type="String"></column>
        <column name="empType" type="String"></column>
	</entity>
</service-builder>